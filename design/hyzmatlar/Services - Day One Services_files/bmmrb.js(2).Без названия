function scrollHandler(){
var thresholdValueDown=-400;
$('.middle-content').each(function (idx){
if((($(this).offset().top - $(window).height()) - ($(window).scrollTop())) <=thresholdValueDown){
var $class=$(this).attr('data-animate')+' animated';$(this).addClass($class);
}});
var scroll=$(window).scrollTop();
if(scroll >=300){
$("body").addClass('sticky-head');
}
if(scroll <=195){
$("body").removeClass('sticky-head');
$("body .stickyHeader header").fadeTo(1000, 1);
}}
function flyingRocket45deg(){
var setTimeAnime=0;
setInterval(function (){
if(setTimeAnime==0){
$(".rocket-45deg").animate({top: -20, left: 0 }, 900);
setTimeAnime=1;
}else if(setTimeAnime==1){
$(".rocket-45deg").animate({top: 0 }, 900);
setTimeAnime=0
}}, 300);
}
function flyingRocket(){
var setTimeAnime=0;
setInterval(function (){
if(setTimeAnime==0){
$("#flyingRocket").animate({bottom: -20 }, 700);
setTimeAnime=1;
}else if(setTimeAnime==1){
$("#flyingRocket").animate({bottom: 0 }, 700);
setTimeAnime=0
}}, 300);
};
