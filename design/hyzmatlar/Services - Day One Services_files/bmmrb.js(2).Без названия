function callback(event){
var element=event.target;
var name=event.type;
var namespace=event.namespace;
var items=event.item.count;
var item=event.item.index;
var pages=event.page.count;
var page=event.page.index;
var size=event.page.size;
console.log(event,item,pages,page,size);
if($(element).attr('data-current')){
var cur=parseInt($(element).attr('data-current'));
if(cur>item){
$(element).attr('data-current',item).parent().removeClass('nextAnim prevAnim');
setTimeout(function(){$(element).parent().addClass('prevAnim');},10);
}else{
$(element).attr('data-current',item).parent().removeClass('nextAnim prevAnim');
setTimeout(function(){$(element).parent().addClass('nextAnim');},10);
}}else{
$(element).attr('data-current',item).parent().removeClass('nextAnim prevAnim').addClass('nextAnim');
setTimeout(function(){$(element).parent().addClass('nextAnim');},10);
}
/*
timeout&&clearTimeout(timeout);
timeout=setTimeout(function(){
$(element).parent().removeClass('nextAnim prevAnim');
}, 2000);*/
}
